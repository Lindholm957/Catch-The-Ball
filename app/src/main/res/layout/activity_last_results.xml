<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tableLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LastResults"
    android:shrinkColumns="*"
    android:stretchColumns="*" >


    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal" >

        <TextView
            android:id="@+id/title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_span="6"
            android:gravity="center"
            android:text="LAST RESULTS INFO"
            android:padding="20dp"
            android:textSize="25dp" >
        </TextView>
    </TableRow>

    <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/get_id"
            android:text="ID"
            android:textStyle="bold"
            android:typeface="serif"
            android:textSize="15sp"
            android:gravity="center">
        </TextView>

        <TextView
            android:id="@+id/get_score"
            android:text="Score"
            android:textStyle="bold"
            android:typeface="serif"
            android:textSize="15sp"
            android:gravity="center">
        </TextView>

        <TextView
            android:id="@+id/get_time"
            android:text="Sesion time"
            android:textStyle="bold"
            android:typeface="serif"
            android:textSize="15sp"
            android:gravity="center">
        </TextView>

        <TextView
            android:id="@+id/get_oranges"
            android:text="Oranges"
            android:textStyle="bold"
            android:typeface="serif"
            android:textSize="15sp"
            android:gravity="center">
        </TextView>

        <TextView
            android:id="@+id/get_pinks"
            android:text="Pinks"
            android:textStyle="bold"
            android:typeface="serif"
            android:textSize="15sp"
            android:gravity="center">
        </TextView>

        <TextView
            android:id="@+id/get_touches"
            android:text="Touches"
            android:textStyle="bold"
            android:typeface="serif"
            android:textSize="15sp"
            android:gravity="center">
        </TextView>
    </TableRow>

    <TableRow
        android:id="@+id/first_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/get_id1"
            android:text="sa"
            android:textStyle="bold"
            android:textSize="15sp"
            android:gravity="center">
        </TextView>

        <TextView
            android:id="@+id/get_score1"
            android:gravity="center_horizontal"
            android:text="28°F" >
        </TextView>

        <TextView
            android:id="@+id/get_time1"
            android:gravity="center_horizontal"
            android:text="26°F" >
        </TextView>

        <TextView
            android:id="@+id/get_oranges1"
            android:gravity="center_horizontal"
            android:text="23°F" >
        </TextView>

        <TextView
            android:id="@+id/get_pinks1"
            android:gravity="center_horizontal"
            android:text="17°F" >
        </TextView>

        <TextView
            android:id="@+id/get_touches1"
            android:gravity="center_horizontal"
            android:text="19°F" >
        </TextView>
    </TableRow>

    <TableRow
        android:id="@+id/second_results"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/get_id2"
            android:text="Day Low"
            android:textStyle="bold" >
        </TextView>

        <TextView
            android:id="@+id/get_score2"
            android:gravity="center_horizontal"
            android:text="15°F" >
        </TextView>

        <TextView
            android:id="@+id/get_time2"
            android:gravity="center_horizontal"
            android:text="10" >
        </TextView>

        <TextView
            android:id="@+id/get_oranges2"
            android:gravity="center_horizontal"
            android:text="3°F" >
        </TextView>

        <TextView
            android:id="@+id/get_pinks2"
            android:gravity="center_horizontal"
            android:text="5°F" >
        </TextView>

        <TextView
            android:id="@+id/get_touches2"
            android:gravity="center_horizontal"
            android:text="6°F" >
        </TextView>
    </TableRow>

    <TableRow
        android:id="@+id/tableRow3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center" >

        <TextView
            android:id="@+id/textView8"
            android:text="Conditions"
            android:textStyle="bold" >
        </TextView>

        <ImageView
            android:id="@+id/imageView1"
            android:src="@drawable/hot" >
        </ImageView>

        <ImageView
            android:id="@+id/imageView2"
            android:src="@drawable/pt_cloud" >
        </ImageView>

        <ImageView
            android:id="@+id/imageView3"
            android:src="@drawable/snow" >
        </ImageView>

        <ImageView
            android:id="@+id/imageView4"
            android:src="@drawable/lt_snow" >
        </ImageView>

        <ImageView
            android:id="@+id/imageView5"
            android:src="@drawable/pt_sun" >
        </ImageView>
    </TableRow>





</TableLayout>